* Définitions des structures
#+BEGIN_SRC C
typedef struct {
    int noad;
    char prenom[15];
    char nom[15];
    Date inscrip;
} Adherent;

typedef struct{
	char nom[30];
	int nbJoueursMax;
	int nbexemplaire;
} Jeu;

typedef struct {
    Adherent *ad;
    int nbemp;
    listeEmp lemp;
} Emprunt;
#+END_SRC
