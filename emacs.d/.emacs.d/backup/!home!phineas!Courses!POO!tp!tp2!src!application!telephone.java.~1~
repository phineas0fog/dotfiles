package application;

public class Telephone {
    String numeroAComposer;
    boolean etatDecroche;

    public void telephoner(){
	if(numeroAComposer==null){
	    System.out.println("Aucun numéro à appeler.");
	}
	if(!etatDecroche){
	    System.out.println("Impossible de composer.");
	}
	
    }
}
