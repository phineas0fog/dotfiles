#prev tab
"xvkbd -text "\{+Control_L}\{+Shift_L}\{+Tab}\{-Tab}\{-Shift_L}\{-Control_L}""
       b:8

#next tab
"xvkbd -text "\{+Control_L}\{+Tab}\{-Tab}\{-Control_L}""
       b:9

#alt
"xvkbd -text "\[F5]""
       b:13

#firefox
"xvkbd -xsendevent -text "\[XF86Back]""
       b:11 
"xvkbd -xsendevent -text "\[XF86Forward]""
       b:12

"xvkbd -text "\{+Alt_L}\{+Tab}\{-Tab}\{-Alt_L}""
       m:0x10 + b:10

